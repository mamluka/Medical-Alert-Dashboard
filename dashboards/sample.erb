<% content_for :title do %>Medical Alert Dashboard
<% end %>
<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="total-sales" data-view="Number" data-title="Total sales" data-moreinfo="today"></div>
    </li>

    <li data-row="1" data-col="2" data-sizex="1" data-sizey="1">
      <div data-id="total-inbound-sales" data-view="Number" data-title="Inbound sales" data-moreinfo="today"></div>
    </li>

    <li data-row="1" data-col="3" data-sizex="1" data-sizey="1">
      <div data-id="total-outbound-sales" data-view="Number" data-title="Outbound sales" data-moreinfo="today"></div>
    </li>


    <li data-row="2" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="confirmed-this-week-sales" data-view="Number" data-title="Confirmed sales" data-moreinfo="this week"></div>
    </li>

    <li data-row="2" data-col="2" data-sizex="1" data-sizey="1">
      <div data-id="unconfirmed-this-week-sales" data-view="Number" data-title="Unconfirmed sales" data-moreinfo="this week"></div>
    </li>

    <li data-row="3" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="confirmed-past-week-sales" data-view="Number" data-title="Confirmed sales" data-moreinfo="past week"></div>
    </li>

    <li data-row="3" data-col="2" data-sizex="1" data-sizey="1">
      <div data-id="unconfirmed-past-week-sales" data-view="Number" data-title="Unconfirmed sales" data-moreinfo="past week"></div>
    </li>

  </ul>
</div>